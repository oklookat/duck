{
  "name": "@oklookat/duck",
  "author": "oklookat",
  "version": "1.0.2",
  "license": "MIT",
  "description": "XHR wrapper",
  "keywords": [
    "XHR",
    "XHR wrapper",
    "request",
    "http"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/oklookat/duck.git"
  },
  "files": [
    "dist"
  ],
  "types": "./dist/types.d.ts",
  "type": "module",
  "browser": "./dist/index.es.js",
  "exports": {
    ".": {
      "import": "./dist/index.es.js"
    }
