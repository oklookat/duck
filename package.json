    "coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@types/node": "^17.0.15",
    "jsdom": "^19.0.0",
    "msw": "^0.36.8",
    "shx": "^0.3.3",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vitest": "^0.2.7"
  }
}
